{
  "name": "@enalmada/start-secure",
  "description": "Security header management for TanStack Start",
  "version": "0.1.0",
  "author": "Adam Lane",
  "bugs": {
    "url": "https://github.com/Enalmada/start-secure/issues"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@changesets/cli": "^2.27.1",
    "@enalmada/bun-externals": "0.0.7",
    "@tanstack/react-start": "^1.103.3",
    "@types/bun": "^1.1.13",
    "@types/node": "^22.10.2",
    "bun-types": "^1.1.38",
    "fixpack": "^4.0.0",
    "lefthook": "^1.13.6",
    "turbo": "^2.5.8",
    "typescript": "^5.6.3",
    "vitest": "^2.1.8"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "homepage": "https://github.com/Enalmada/start-secure#readme",
  "keywords": [
    "content-security-policy",
    "csp",
    "headers",
    "security",
    "tanstack",
    "tanstack-start"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "packageManager": "bun@1.2.23",
  "peerDependencies": {
    "@tanstack/react-start": ">=1.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Enalmada/start-secure.git"
  },
  "scripts": {
    "build": "bun run type-check && bun run clean && bun run build:bundle && bun run build:declaration",
    "build:bundle": "bun build.ts",
    "build:declaration": "tsc --emitDeclarationOnly",
    "check": "turbo lint type-check test",
    "clean": "node -e \"require('fs').rmSync('dist', {recursive: true, force: true})\"",
    "lint": "biome check --fix --unsafe .",
    "prepare": "lefthook install",
    "release": "bun run build && changeset publish",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit"
  },
  "sideEffects": false,
  "types": "./dist/index.d.ts"
}
